<table class="table">
  <thead>
    <tr>
      <th> ID </th>
      <th> Name </th>
      <th> Type </th>
      <th> Desc </th>
      <th> User </th>
      <th> Tag </th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td> <%= @graph.id %> </td>
      <td> <%= @graph.name %> </td>
      <td> <%= @graph.description %> </td>
      <td> <%= @graph.g_type %> </td>
      <td> <%= @graph.user.email %> </td>
      <td> <%= @graph.tag %> </td>
      <td>
        <%= link_to "edit", edit_graph_path(@graph) %> 
      </td>
    </tr>
  </tbody>
</table>

<%= react_component 'ChartContainer', options: @graph_config %>

<div> 
  <pre> <%= @graph.raw_graph_settings %> </pre>
</div>
